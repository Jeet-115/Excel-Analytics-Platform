    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.14.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "uuid": "^9.0.0",
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
