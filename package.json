  "description": "Sheet Sense - AI-powered Excel Analysis Platform with 2D/3D charts, AI insights, and multilingual support",
  "main": "backend/index.js",
  "scripts": {
    "start:server": "node server/server.js",
    "start:frontend": "cd client && npm run dev",
